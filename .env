# For Windows user
# set the path to your project folder
#PWD=C:\Users\fabian vogel\Downloads\OXID_Docker_Stack-master

## Shop settings
# Shop Version 6.0, 6.1
OXID_VERSION=6.1
# Shop Edition ce, pe, ee, b2b
OXID_EDITION=ce

# Your OXID Serial
OXID_SERIAL=XXXXX-XXXXX-XXXXX-XXXXX-XXXXX-XXXXX

# Demodata true or false
OXID_DEMODATA=true

# set to false if you want to run the setup manually
INSTALL_SHOP=true

# Shopdomain
DOMAIN=localhost

# httpd or nginx
WEBSERVER=httpd

#memcached enabled true or false
MEMCACHED=true

# Set ports
WEBSERVER_PORT=80
MAILHOG_PORT=8025
ADMINER_PORT=8080


# Set PHP version
# PHP Versions 7.0, 7.1, 7.2, 7.3
PHP_VERSION=7.1


# DB Settings
# MySQL Images mysql:5.5, mysql:5.6, mysql:5.7, mysql:8
# MariaDB Images mariadb:5.5, mariadb:10.0, mariadb:10.1, mariadb:10.2, mariadb:10.3
DB_IMAGE=mysql:5.7
MYSQL_DATABASE=oxid
MYSQL_USER=oxid
MYSQL_PASSWORD=oxid
MYSQL_ROOT_PASSWORD=root